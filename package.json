{
  "name": "redest",
  "version": "3.0.4",
  "description": "Rest Api communication made easy",
  "main": "distribution/index.js",
  "repository": "git@github.com:momentechnologies/redest.git",
  "author": "Max Moeschinger <m.moeschinger@momentech.no>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --ignore __tests__ --out-dir distribution",
    "dev": "watch \"npm run build\" src/",
    "prepublish": "npm run build",
    "test": "jest",
    "prettier": "prettier --write \"{config,src}/**/*.{jsx,js,json,scss}\"",
    "eslint": "eslint src",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss}": [
      "prettier",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.3.0",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "lint-staged": "^6.0.0",
    "nodemon": "^1.12.0",
    "prettier": "^1.7.4",
    "regenerator-runtime": "^0.11.0",
    "remove": "^0.1.5"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "deepmerge": "^2.0.1",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-redux": "^5.0.6",
    "reselect": "^3.0.0"
  }
}
